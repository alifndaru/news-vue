{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginItem\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  methods: {\n    login() {\n      const apiBaseUrl = \"http://localhost:8080\"; // Ganti dengan URL sesuai dengan backend Anda\n\n      // Panggil endpoint /sanctum/csrf-cookie terlebih dahulu\n      axios.get(`${apiBaseUrl}/sanctum/csrf-cookie`).then(() => {\n        const formData = {\n          email: this.email,\n          password: this.password\n        };\n        axios.post(`${apiBaseUrl}/api/login`, formData).then(response => {\n          if (response.data.access_token) {\n            localStorage.setItem(\"token\", response.data.access_token);\n            this.$router.push(\"/home\");\n          }\n        }).catch(error => {\n          console.error(\"Login error:\", error);\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","methods","login","apiBaseUrl","get","then","formData","post","response","access_token","localStorage","setItem","$router","push","catch","error","console"],"sources":["/home/alif-ndaru/Documents/Workspace/project-test/frontend-app/src/view/auth/Login.vue"],"sourcesContent":["<template>\n  <div class=\"login-container\">\n    <h2>Login</h2>\n    <form @submit.prevent=\"login\">\n      <div class=\"form-group\">\n        <label for=\"email\">Email</label>\n        <input type=\"email\" id=\"email\" v-model=\"email\" required />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"password\">Password</label>\n        <input type=\"password\" id=\"password\" v-model=\"password\" required />\n      </div>\n      <button type=\"submit\">Login</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginItem\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n    };\n  },\n  methods: {\n    login() {\n      const apiBaseUrl = \"http://localhost:8080\"; // Ganti dengan URL sesuai dengan backend Anda\n\n      // Panggil endpoint /sanctum/csrf-cookie terlebih dahulu\n      axios.get(`${apiBaseUrl}/sanctum/csrf-cookie`).then(() => {\n        const formData = {\n          email: this.email,\n          password: this.password,\n        };\n\n        axios\n          .post(`${apiBaseUrl}/api/login`, formData)\n          .then((response) => {\n            if (response.data.access_token) {\n              localStorage.setItem(\"token\", response.data.access_token);\n              this.$router.push(\"/home\");\n            }\n          })\n          .catch((error) => {\n            console.error(\"Login error:\", error);\n          });\n      });\n    },\n  },\n};\n</script>\n\n<style>\n/* ... gaya tampilan seperti sebelumnya ... */\n</style>\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,MAAMC,UAAS,GAAI,uBAAuB,EAAE;;MAE5C;MACAP,KAAK,CAACQ,GAAG,CAAE,GAAED,UAAW,sBAAqB,CAAC,CAACE,IAAI,CAAC,MAAM;QACxD,MAAMC,QAAO,GAAI;UACfP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC;QAEDJ,KAAI,CACDW,IAAI,CAAE,GAAEJ,UAAW,YAAW,EAAEG,QAAQ,EACxCD,IAAI,CAAEG,QAAQ,IAAK;UAClB,IAAIA,QAAQ,CAACV,IAAI,CAACW,YAAY,EAAE;YAC9BC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACV,IAAI,CAACW,YAAY,CAAC;YACzD,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;UAC5B;QACF,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;UAChBC,OAAO,CAACD,KAAK,CAAC,cAAc,EAAEA,KAAK,CAAC;QACtC,CAAC,CAAC;MACN,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}